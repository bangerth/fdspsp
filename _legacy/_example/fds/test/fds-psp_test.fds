&HEAD CHID='fds-psp_test' /

&MESH ID='1' IJK=40,20,10, XB=0.0,8.0,0.0,4.0,0.0,2.0 /
&MESH ID='2' IJK=40,20,10, XB=0.0,8.0,0.0,4.0,2.0,4.0 /

&TIME T_END=2. /

&MISC PARTICLE_CFL=.TRUE., POROUS_FLOOR=.FALSE. /

!! =========
!! Particles
!! =========
&SPEC ID='WATER VAPOR' /
&REAC FUEL='N-HEPTANE', CO_YIELD=0.0, SOOT_H_FRACTION=0.1, SOOT_YIELD=0.015 /

&PART ID='water droplets', SPEC_ID='WATER VAPOR',
      QUANTITIES(1:7)='PARTICLE DIAMETER', 'PARTICLE MASS', 'PARTICLE TEMPERATURE', 'PARTICLE U', 'PARTICLE V', 'PARTICLE W', 'PARTICLE WEIGHTING FACTOR',
      DIAMETER=300., SAMPLING_FACTOR=1 / 

&PART ID='heptane droplets', SPEC_ID='N-HEPTANE',
      QUANTITIES(1:7)='PARTICLE DIAMETER', 'PARTICLE MASS', 'PARTICLE TEMPERATURE', 'PARTICLE U', 'PARTICLE V', 'PARTICLE W', 'PARTICLE WEIGHTING FACTOR',
      DIAMETER=100., HEAT_OF_COMBUSTION=44500., SAMPLING_FACTOR=1 /

&DEVC ID='nozzle_1', XYZ=4.0,3.4,1.0, PROP_ID='nozzle', QUANTITY='TIME', SETPOINT=0., ORIENTATION=0,-1,0 /

&PROP ID='nozzle', PART_ID='water droplets', PARTICLES_PER_SECOND=10000, FLOW_RATE=102.,  DROPLET_VELOCITY=250., SPRAY_ANGLE=70.,90., OFFSET=0.125, FLOW_RAMP='fuel' / 
&RAMP ID='fuel', T=0.0, F=1.0 /
&RAMP ID='fuel', T=1.0, F=0.0 /
&RAMP ID='fuel', T=2.0, F=0.0 /

!! ============
!! Obstructions
!! ============
&OBST XB= 3.0,5.0,3.5,4.0,0.0,2.0 /

!! ==========
!! Boundaries
!! ==========
&VENT MB='XMIN', SURF_ID='OPEN' /
&VENT MB='XMAX', SURF_ID='OPEN' /
&VENT MB='YMIN', SURF_ID='OPEN' /
&VENT MB='YMAX', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&TAIL /
